    ["Shader Color"] = "ç€è‰²å™¨é¢œè‰²",
    ["Lighting Theme"] = "ç…§æ˜ä¸»é¢˜",
    ["Violet"] = "ç´«ç½—å…°è‰²",
    ["Red"] = "çº¢è‰²",
    ["Save"] = "ä¿å­˜",
    ["Saves elements to config"] = "å°†å…ƒç´ ä¿å­˜åˆ°é…ç½®",
    ["Load"] = "åŠ è½½",
    ["Loads elements from config"] = "ä»é…ç½®åŠ è½½å…ƒç´ ",
    ["Performance"] = "æ€§èƒ½",
    ["Teleport"] = "ä¼ é€",
    ["Visuals"] = "è§†è§‰æ•ˆæœ",
    ["Local Player"] = "æœ¬åœ°ç©å®¶",
    ["Misc"] = "æ‚é¡¹",
